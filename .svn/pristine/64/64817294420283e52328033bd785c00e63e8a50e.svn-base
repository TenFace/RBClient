<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/head_layout"
    android:layout_width="match_parent"
    android:layout_height="50dp"
    android:background="@drawable/head_bg"
    android:gravity="center_vertical" >
    <RelativeLayout
        android:id="@+id/home_Toback"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <Button
            android:id="@+id/head_left"
            style="@style/style_15_EBEBEB_BOLD"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="7dip"
            android:background="@drawable/new_head_back_selector"
            android:clickable="true"
            android:gravity="center"
            android:text="@string/goback" />
        <TextView
            style="@style/style_18_BEBEBE_BOLD"
            android:layout_width="wrap_content"
            android:id="@+id/head_title"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            />

    </RelativeLayout>


    <Button
        android:id="@+id/head_right"
        style="@style/style_15_EBEBEB_BOLD"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_gravity="right"
        android:layout_marginRight="7dip"
        android:background="@drawable/new_head_normal_selector"
        android:gravity="center"
        android:visibility="invisible"
          />

</RelativeLayout>