    <style name="title_btn_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:padding">5dp</item>
        <item name="android:background">@drawable/bar_head_button_bg</item>
        <item name="android:textColor">#FFF</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="more_tab_tv_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">12dp</item>
        <item name="android:textColor">#000</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="more_tab_iv_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_marginRight">8dp</item>
        <item name="android:background">@null</item>
    </style>

    <style name="more_tab_rl_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:padding">12dp</item>
        <item name="android:layout_margin">6dp</item>
        <item name="android:clickable">false</item>
        <item name="android:background">@drawable/moro_tab_bg</item>
    </style>

    <style name="username_tab_tv_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">16dp</item>
        <item name="android:layout_marginBottom">5dp</item>
        <item name="android:textColor">#77000000</item>
        <item name="android:textSize">13sp</item>
    </style>

    <style name="service_telephone_btn_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentBottom">true</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_marginBottom">20dp</item>
        <item name="android:background">@drawable/product_detail_shop_normal</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:textColor">#FFF</item>
    </style>

    <style name="Detail_Total">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#000</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:textSize">10dp</item>
    </style>

    <style name="Detail_Goods">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#ff7a7a7a</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:textSize">14dp</item>
    </style>

    <!--订单详情页面-->
    <style name="orderdetail_content_left">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#000</item>
        <item name="android:textSize">14dp</item>
    </style>

    <style name="orderdetail_content_right">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#666</item>
        <item name="android:textSize">14dp</item>
    </style>

    <style name="address_ll_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">10dp</item>
        <item name="android:background">@drawable/middle</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="address_et_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:background">@null</item>
        <item name="android:hint">(必填)</item>
    </style>

    <!-- 分享对话框样式 -->
    <style name="ShareDialog" parent="@android:Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

</resources>